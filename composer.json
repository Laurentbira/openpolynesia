    "require-dev": {
        "composer/installers": "^2.2",
        "cweagans/composer-patches": "^1.7.2",
        "drupal/coder": "^8.3",
        "drupal/config_inspector": "^2.1.0",
        "drupal/core-composer-scaffold": "^9.5.0",
        "drupal/core-dev": "^9.5.0",
        "drupal/core-recommended": "^9.5.0",
        "drupal/devel": "^5.1",
        "drupal/stage_file_proxy": "^2.0",
        "drupal/upgrade_status": "^3.18",
        "drush/drush": "^11.4",
        "oomphinc/composer-installers-extender": "^2.0.1",
        "openculturas/openculturas-profile": "self.version",
        "palantirnet/drupal-rector": "^0.13.1",
        "phpspec/prophecy-phpunit": "^2",
        "phpstan/extension-installer": "^1.2",
        "weitzman/drupal-test-traits": "^2.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\openculturas\\" : "tests/src"
        }
    },
    "conflict": {
        "drupal/drupal": "*",
        "drupal/ctools": "~4.0.0"
    },
    "replace": {
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        },
        "platform": {
            "php": "7.4.21"
        }
    },
    "scripts": {
        "update-config": "@php scripts/update_config.php",
        "export-content": [
            "drush pm:install default_content --yes",
            "rm -rf web/profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush default-content:export-references node --folder=profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush default-content:export-references media --folder=profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush default-content:export-references menu_link_content --folder=profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush default-content:export-references taxonomy_term --folder=profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush pm:uninstall default_content hal --yes"
        ],
        "si": [
            "rm -rf web/sites/default/files && mkdir -p web/sites/default/files/translations",
            "drush site:install --yes"
        ],
        "php:cs": "phpcs -p -s --warning-severity=0",
        "php:cs-fix": "phpcbf",
        "php:phpstan": "phpstan",
        "php:rc-dry": "rector process --dry-run",
        "php:rc": "rector process"
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[project-root]/.gitattributes": false,
                "[web-root]/example.gitignore": false,
                "[web-root]/INSTALL.txt": false,
                "[web-root]/README.md": false,
                "[web-root]/.gitignore": false,
                "[web-root]/sites/README.txt": false,
                "[web-root]/modules/README.txt": false,
                "[web-root]/profiles/README.txt": false,
                "[web-root]/themes/README.txt": false,
                "[web-root]/sites/development.services.yml": {
                    "mode": "replace",
                    "path": "assets/development.services.yml",
                    "overwrite": false
                }
            }
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "vendor:npm-asset",
                "vendor:bower-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/flag": {
                "Issue #3260640: A11y: Send the status message via drupal.announce": "https://www.drupal.org/files/issues/2022-12-22/flag-3260640-ajax-link-message-announce-8.patch",
                "Issue #3143606: missing global value when saving flagging with form field entry": "https://www.drupal.org/files/issues/2020-05-28/flagging_global-3143606-3.patch",
                "Issue #3059976: 'Unflag not allowed text' not showing up": "https://www.drupal.org/files/issues/2022-12-01/flag-not-allowed-text-3059976-12.patch"
            },
            "drupal/focal_point": {
                "Issue #2830678: Limit image styles on preview page": "https://www.drupal.org/files/issues/2021-08-13/2830678-29.patch"
            },
            "drupal/views_infinite_scroll": {
                "Issue #3267139: Make Load more markup more accessible": "https://www.drupal.org/files/issues/2022-07-18/views_infinite_scroll-3267139-missing-pager-headline-10.patch"
            },
            "drupal/core": {
                "Remove color module logo replacement logic": "patches/drupal-core-color-logo.patch",
                "Issue #2858897: Block Name Collision on Theme Creation": "https://www.drupal.org/files/issues/2022-12-16/2858897-40.patch",
                "Issue #2818017: A number of Views combined fields filter operators are currently pointless to an end user": "https://www.drupal.org/files/issues/2023-02-07/2818017-30.patch",
                "Issue #2706241: AccessAwareRouter does not respect HTTP method": "https://www.drupal.org/files/issues/2022-02-01/2706241-67.patch",
                "Issue #2321071: BaseFieldOverride fails to take into account ContentEntityInterface::bundleFieldDefinitions() when invoking onFieldDefinitionUpdate()": "https://www.drupal.org/files/issues/2019-12-24/2321071-39.patch",
                "Issue #2911208: Add support for details/vertical tabs ": "https://www.drupal.org/files/issues/2022-07-14/tour-support_fieldsets_vertical_tabs-2911208-16.patch",
                "Issue #2613222: Language module shouldn't alter config in sync mode": "https://www.drupal.org/files/issues/2019-11-06/2613222-13.patch",
                "Issue #3221375: Wrong language field labels after drupal_rebuild()": "https://www.drupal.org/files/issues/2021-12-01/3221375-27-workaround.patch",
                "Issue #2845144: User can't reference unpublished content even when they have access to it": "https://www.drupal.org/files/issues/2022-06-17/2845144-67.patch"
            },
            "drupal/entity_reference_revisions": {
                "Issue #2799479: Views doesn't recognize relationship to host": "https://git.drupalcode.org/project/entity_reference_revisions/-/merge_requests/5.patch"
            },
            "drupal/views_autocomplete_filters": {
                "Issue #2999501: Undefined index: autocomplete_field breaking search view ajax": "https://www.drupal.org/files/issues/2022-03-22/views_autocomplete_filter-2999501-9-stable-dont_break_ajax_with_php_notice.patch"
            },
            "drupal/legal": {
                "Issue #3074688: Password reset links no longer work": "https://git.drupalcode.org/project/legal/-/merge_requests/7.patch",
                "Issue #3252838/3341867: session_destroy(): Trying to destroy uninitialized session": "https://www.drupal.org/files/issues/2023-02-15/3252838-only-fix.patch"
            },
            "drupal/field_group": {
                "Issue #2787179: Ensure visibility of invalid fields (JS)": "https://www.drupal.org/files/issues/2021-09-03/2787179-highlight-html5-validation-74.patch"
            },
            "drupal/formtips": {
                "Issue #3292882: Filter tips are hardly readable in GIN": "https://git.drupalcode.org/project/formtips/-/merge_requests/3.diff",
                "Issue #3248774: Add Accessibility to the FormTips module": "https://www.drupal.org/files/issues/2022-07-14/fix-accessibility-formtips-3248774_16.patch"
            },
            "drupal/addtocal_augment": {
                "Issue #3313492: Set clear option for token": "https://www.drupal.org/files/issues/2022-10-04/addtocal_augment-3313492-token-clear-option.patch",
                "Issue #3314170: Event title can not be set": "https://git.drupalcode.org/project/addtocal_augment/-/merge_requests/5.diff"
            },
            "drupal/asset_injector": {
                "Issue #3082195: Support loading ACE editor locally w/o CDN": "https://www.drupal.org/files/issues/2022-11-23/3082195-19.patch"
            }
        }
    }
}
