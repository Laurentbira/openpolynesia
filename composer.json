    "name": "openculturas/openculturas-distribution",
    "description": "The OpenCulturas distribution install profile.",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Stefan Auditor",
            "email": "stefan.auditor@cowain.de"
        },
        {
            "name": "Tobias BÃ¤hr",
            "email": "tobias.baehr@cowain.de"
