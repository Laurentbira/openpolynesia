                "dist": {
                    "url": "https://github.com/Leaflet/Leaflet.markercluster/archive/refs/tags/v1.4.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "choices-js/choices.js",
                "version": "v10.1.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/Choices-js/Choices/archive/refs/tags/v10.1.0.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "heiseonline/shariff",
                "version": "3.2.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/heiseonline/shariff/releases/download/3.2.1/shariff-3.2.1.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "choices-js/choices.js": "^10.1",
        "drupal/address": "^1.11",
        "drupal/address_display": "^1.0@beta",
        "drupal/addtocal_augment": "^1.0",
        "drupal/allowed_formats": "^1.5",
        "drupal/asset_injector": "^2.12",
        "drupal/attribution": "^1.0",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/better_exposed_filters": "^5.2",
        "drupal/block_field": "^1.0@RC",
        "drupal/choices": "^2.0.4",
        "drupal/coffee": "^1.2",
        "drupal/composer_deploy": "^1.7",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/cookies": "^1.1",
        "drupal/date_augmenter": "^1.0",
        "drupal/date_range_formatter": "^4.0",
        "drupal/datetime_extras": "^1.0@alpha",
        "drupal/default_content": "^2.0@alpha",
        "drupal/duration_field": "^2.0",
        "drupal/entity_browser": "^2.8",
        "drupal/extra_field": "^2.3",
        "drupal/facets": "^2.0.5",
        "drupal/field_formatter_class": "^1.5",
        "drupal/field_group": "^3.3",
        "drupal/field_permissions": "^1.2",
        "drupal/field_time": "^1.0@beta",
        "drupal/flag": "^4.0@beta",
        "drupal/focal_point": "^1.5",
        "drupal/formtips": "^1.5",
        "drupal/geocoder": "^3.31",
        "drupal/geofield": "^1.46",
        "drupal/gin": "^3.0@beta",
        "drupal/gin_toolbar": "^1.0@beta",
        "drupal/honeypot": "^2.1.2",
        "drupal/http_client_error_status": "^3.0",
        "drupal/jquery_ui_accordion": "^1.1",
        "drupal/json_ld_schema": "^1.3",
        "drupal/languagefield": "^1.8",
        "drupal/leaflet": "^2.2.10",
        "drupal/legal": "^2.0",
        "drupal/licenses": "^2.0.2",
        "drupal/linkit": "^6.0@beta",
        "drupal/matomo": "^1.19",
        "drupal/media_library_edit": "^2.3",
        "drupal/metatag": "^1.22",
        "drupal/oembed_providers": "^2.1",
        "drupal/paragraphs": "^1.15",
        "drupal/pathauto": "^1.11",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/realname": "^2.0@beta",
        "drupal/redirect": "^1.8",
        "drupal/role_delegation": "^1.2",
        "drupal/search_api": "^1.27",
        "drupal/shariff": "^1.7",
        "drupal/similarterms": "^1.5",
        "drupal/simple_sitemap": "^4.1.3",
        "drupal/smart_date": "^3.6.1",
        "drupal/smart_trim": "^2.0",
        "drupal/smtp": "^1.0",
        "drupal/social_media_links": "^2.9",
        "drupal/svg_image": "^1.16",
        "drupal/token": "^1.11",
        "drupal/tour_enhancements": "^1.0.1",
        "drupal/tour_ui": "^1.0@RC",
        "drupal/transliterate_filenames": "^2.0.1",
        "drupal/twig_tweak": "^3.2",
        "drupal/view_password": "^6.0",
        "drupal/viewfield": "^3.0@beta",
        "drupal/views_add_button": "^2.0.2",
        "drupal/views_autocomplete_filters": "^1.3",
        "drupal/views_bulk_edit": "^2.8",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/views_data_export": "^1.2",
        "drupal/views_infinite_scroll": "^2.0.1",
        "dynamicweb/swiffy-slider": "^1.6",
        "elmarquis/leaflet.gesture-handling": "^1.2.2",
        "geocoder-php/nominatim-provider": "^5.5.1",
        "heiseonline/shariff": "^3.2.1",
        "leaflet/leaflet.fullscreen": "^1.0.2",
        "leaflet/leaflet.markercluster": "^1.4.1",
        "npm-asset/font-awesome": "^6.2.0"
    },
    "require-dev": {
        "composer/installers": "^2.2",
        "cweagans/composer-patches": "^1.7.2",
        "drupal/config_inspector": "^2.1.0",
        "drupal/core-composer-scaffold": "^9.4.8",
        "drupal/core-dev-pinned": "^9.4.8",
        "drupal/core-recommended": "^9.4.8",
        "drupal/devel": "^5.0",
        "drupal/stage_file_proxy": "^1.2",
        "drush/drush": "^11.3.2",
        "oomphinc/composer-installers-extender": "^2.0.1",
        "openculturas/openculturas-profile": "*",
        "weitzman/drupal-test-traits": "^1.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\openculturas\\" : "tests/src"
        }
    },
    "conflict": {
        "drupal/drupal": "*",
        "drupal/ctools": "~4.0.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "platform": {
            "php": "7.4.21"
        }
    },
    "scripts": {
        "update-config": "@php scripts/update_config.php",
        "export-content": [
            "drush pm:install default_content --yes",
            "rm -rf web/profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush default-content:export-references node --folder=profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush default-content:export-references media --folder=profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush default-content:export-references menu_link_content --folder=profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush default-content:export-references taxonomy_term --folder=profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush pm:uninstall default_content hal --yes"
        ],
        "si": [
            "rm -rf web/sites/default/files && mkdir -p web/sites/default/files/translations",
            "drush site:install --yes"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[project-root]/.gitattributes": false,
                "[web-root]/example.gitignore": false,
                "[web-root]/INSTALL.txt": false,
                "[web-root]/README.md": false,
                "[web-root]/.gitignore": false,
                "[web-root]/sites/README.txt": false,
                "[web-root]/modules/README.txt": false,
                "[web-root]/profiles/README.txt": false,
                "[web-root]/themes/README.txt": false,
                "[web-root]/sites/development.services.yml": {
                    "mode": "replace",
                    "path": "assets/development.services.yml",
                    "overwrite": false
                }
            }
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "installer-paths": {
            "profile/themes/openculturas_base/fontawesome6": [
                "npm-asset/font-awesome"
            ],
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "vendor:npm-asset",
                "vendor:bower-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/flag": {
                "Issue #3165081: Destination URL not set when deleting flag through Field Entry Form": "https://www.drupal.org/files/issues/2020-09-28/flag-destination-url-not-set-3165081-2.patch",
                "Issue #3260640: A11y: Send the status message via drupal.announce": "https://www.drupal.org/files/issues/2022-01-26/flag-3260640-ajax-link-message-announce-3.patch",
                "Issue #3143606: missing global value when saving flagging with form field entry": "https://www.drupal.org/files/issues/2020-05-28/flagging_global-3143606-3.patch",
                "Issue #3059976: 'Unflag not allowed text' not showing up": "https://www.drupal.org/files/issues/2020-02-13/flag-not-allowed-text-3059976-3.patch"
            },
            "drupal/focal_point": {
                "Issue #2830678: Limit image styles on preview page": "https://www.drupal.org/files/issues/2021-08-13/2830678-29.patch"
            },
            "drupal/views_infinite_scroll": {
                "Issue #3267139: Make Load more markup more accessible": "https://www.drupal.org/files/issues/2022-07-18/views_infinite_scroll-3267139-missing-pager-headline-10.patch"
            },
            "drupal/core": {
                "Remove color module logo replacement logic": "patches/drupal-core-color-logo.patch",
                "Issue #2858897: Block Name Collision on Theme Creation": "https://www.drupal.org/files/issues/2022-01-17/2858897-29.patch",
                "Issue #2818017: A number of Views combined fields filter operators are currently pointless to an end user": "https://www.drupal.org/files/issues/2021-12-20/2818017-19.patch",
                "Issue #2706241: AccessAwareRouter does not respect HTTP method": "https://www.drupal.org/files/issues/2022-02-01/2706241-67.patch",
                "Issue #2321071: BaseFieldOverride fails to take into account ContentEntityInterface::bundleFieldDefinitions() when invoking onFieldDefinitionUpdate()": "https://www.drupal.org/files/issues/2019-12-24/2321071-39.patch",
                "Issue #2911208: Add support for details/vertical tabs ": "https://www.drupal.org/files/issues/2022-07-14/tour-support_fieldsets_vertical_tabs-2911208-16.patch",
                "Issue #2613222: Language module shouldn't alter config in sync mode": "https://www.drupal.org/files/issues/2019-11-06/2613222-13.patch",
                "Issue #3221375: Wrong language field labels after drupal_rebuild()": "https://www.drupal.org/files/issues/2021-12-01/3221375-27-workaround.patch"
            },
            "drupal/entity_reference_revisions": {
                "Issue #2799479: Views doesn't recognize relationship to host": "https://git.drupalcode.org/project/entity_reference_revisions/-/merge_requests/5.patch"
            },
            "drupal/views_autocomplete_filters": {
                "Issue #2999501: Undefined index: autocomplete_field breaking search view ajax": "https://www.drupal.org/files/issues/2022-03-22/views_autocomplete_filter-2999501-9-stable-dont_break_ajax_with_php_notice.patch"
            },
            "drupal/legal": {
                "Issue #3074688: Password reset links no longer work": "https://git.drupalcode.org/project/legal/-/merge_requests/7.patch",
                "Issue #3252838: session_destroy(): Trying to destroy uninitialized session": "https://git.drupalcode.org/project/legal/-/merge_requests/4.patch"
            },
            "drupal/field_group": {
                "Issue #2787179: Ensure visibility of invalid fields (JS)": "https://www.drupal.org/files/issues/2021-09-03/2787179-highlight-html5-validation-74.patch"
            },
            "drupal/duration_field": {
                "Issue #3284489: CSS Styles for duration field are not applied in some themes.": "https://www.drupal.org/files/issues/2022-06-07/duration_field-3284489.patch"
            },
            "drupal/formtips": {
                "Issue #3292882: Filter tips are hardly readable in GIN": "https://git.drupalcode.org/project/formtips/-/merge_requests/3.diff",
                "Issue #3248774: Add Accessibility to the FormTips module": "https://www.drupal.org/files/issues/2022-07-14/fix-accessibility-formtips-3248774_16.patch"
            },
            "drupal/addtocal_augment": {
                "Issue #3313492: Set clear option for token": "https://www.drupal.org/files/issues/2022-10-04/addtocal_augment-3313492-token-clear-option.patch",
                "Issue #3314170: Event title can not be set": "https://git.drupalcode.org/project/addtocal_augment/-/merge_requests/5.diff"
            }
        }
    }
}
