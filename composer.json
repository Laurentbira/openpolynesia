        "npm-asset/font-awesome": "^6.0"
    },
    "require-dev": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/config_inspector": "^2.0",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-dev-pinned": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drupal/devel": "^4.1",
        "drupal/stage_file_proxy": "^1.2",
        "drush/drush": "^11.0",
        "npm-asset/jquery-ui-touch-punch": "^0.2.3",
        "oomphinc/composer-installers-extender": "^2.0",
        "openculturas/openculturas-profile": "*",
        "weitzman/drupal-test-traits": "dev-master"
    },
    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\openculturas\\" : "tests/src"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "scripts": {
        "update-config": "@php scripts/update_config.php",
        "export-content": [
            "drush pm:install default_content --yes",
            "rm -rf web/profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush default-content:export-references node --folder=profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush default-content:export-references media --folder=profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush default-content:export-references menu_link_content --folder=profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush default-content:export-references taxonomy_term --folder=profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush pm:uninstall default_content --yes"
        ],
        "si": [
            "rm -rf web/sites/default/files && mkdir -p web/sites/default/files/translations",
            "drush site:install --yes"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[project-root]/.gitattributes": false,
                "[web-root]/example.gitignore": false,
                "[web-root]/INSTALL.txt": false,
                "[web-root]/README.md": false,
                "[web-root]/.gitignore": false,
                "[web-root]/sites/README.txt": false,
                "[web-root]/modules/README.txt": false,
                "[web-root]/profiles/README.txt": false,
                "[web-root]/themes/README.txt": false,
                "[web-root]/sites/development.services.yml": {
                    "mode": "replace",
                    "path": "assets/development.services.yml",
                    "overwrite": false
                }
            }
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "installer-paths": {
            "profile/themes/openculturas_base/fontawesome6": [
                "npm-asset/font-awesome"
            ],
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "vendor:npm-asset",
                "vendor:bower-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/smtp": {
                "Issue #3248547: Remove after install messages": "https://www.drupal.org/files/issues/2021-11-09/3248547-2-remove-after-install-messages.patch"
            },
            "drupal/flag": {
                "Issue #3165081: Destination URL not set when deleting flag through Field Entry Form": "https://www.drupal.org/files/issues/2020-09-28/flag-destination-url-not-set-3165081-2.patch",
                "Issue #3260640: A11y: Send the status message via drupal.announce": "https://www.drupal.org/files/issues/2022-01-26/flag-3260640-ajax-link-message-announce-3.patch"
            },
            "drupal/focal_point": {
                "Issue #2830678: Limit image styles on preview page": "https://www.drupal.org/files/issues/2021-08-13/2830678-29.patch"
            },
            "drupal/address_display": {
                "Issue #3260530: Config schema for field formatter is missing": "https://www.drupal.org/files/issues/2022-01-26/address_display-3260530-2.patch"
            },
            "drupal/views_infinite_scroll": {
                "Issue #3267139: Make Load more markup more accessible": "https://www.drupal.org/files/issues/2022-03-10/views_infinite_scroll-3267139-missing-pager-headline-9.patch"
            },
            "drupal/core": {
                "Issue #2858897: Block Name Collision on Theme Creation": "https://www.drupal.org/files/issues/2022-01-17/2858897-29.patch",
                "Issue #2818017: A number of Views combined fields filter operators are currently pointless to an end user": "https://www.drupal.org/files/issues/2021-12-20/2818017-19.patch",
                "Issue #2706241: AccessAwareRouter does not respect HTTP method": "https://www.drupal.org/files/issues/2022-02-01/2706241-67.patch"
            },
            "drupal/entity_reference_revisions": {
                "Issue #2799479: Views doesn't recognize relationship to host": "https://git.drupalcode.org/project/entity_reference_revisions/-/merge_requests/5.patch"
            },
            "drupal/geofield": {
                "Issue #3254909: Simplify Proximity Filter": "https://git.drupalcode.org/project/geofield/-/merge_requests/9.patch"
            },
            "drupal/views_autocomplete_filters": {
                "Issue #2999501: Undefined index: autocomplete_field breaking search view ajax": "https://www.drupal.org/files/issues/2022-03-22/views_autocomplete_filter-2999501-9-stable-dont_break_ajax_with_php_notice.patch"
            },
            "drupal/legal": {
                "Issue #3074688: Password reset links no longer work": "https://git.drupalcode.org/project/legal/-/merge_requests/7.patch",
                "Issue #3252838: session_destroy(): Trying to destroy uninitialized session": "https://git.drupalcode.org/project/legal/-/merge_requests/4.patch"
            },
            "drupal/field_group": {
                "Issue #2787179: Ensure visibility of invalid fields (JS)": "https://www.drupal.org/files/issues/2021-09-03/2787179-highlight-html5-validation-74.patch"
            }
        }
    }
}
