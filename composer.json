{
    "name": "openculturas/openculturas-distribution",
    "description": "The OpenCulturas distribution install profile.",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Stefan Auditor",
            "email": "stefan.auditor@cowain.de"
        },
        {
            "name": "Tobias BÃ¤hr",
            "email": "tobias.baehr@cowain.de"
        },
        {
            "name": "Meike Jung",
            "email": "meike.jung@cowain.de"
        },
        {
            "name": "Peter Gerken",
            "email": "peter.gerken@erdfisch.de"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "path",
            "url": "profile"
        }
    ],
    "require": {
        "drupal/address": "^1.9",
        "drupal/address_display": "^1.0@beta",
        "drupal/allowed_formats": "^1.4",
        "drupal/attribution": "^1.0",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/coffee": "^1.2",
        "drupal/composer_deploy": "^1.6",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/date_range_formatter": "^9.0-dev",
        "drupal/duration_field": "^2.0",
        "drupal/elasticsearch_connector": "^7.0-dev",
        "drupal/entity_browser": "^2.6",
        "drupal/eu_cookie_compliance": "^1.19",
        "drupal/facets": "^2.0",
        "drupal/feeds": "^3.0-dev",
        "drupal/field_group": "^3.2",
        "drupal/field_time": "^1.0@beta",
        "drupal/flag": "^4.0@beta",
        "drupal/focal_point": "^1.5",
        "drupal/geocoder": "^3.24",
        "drupal/geofield": "^1.35",
        "drupal/gin": "@dev",
        "drupal/gin_toolbar": "@dev",
        "drupal/group": "^1.4",
        "drupal/jquery_ui_accordion": "^1.1",
        "drupal/json_ld_schema": "^1.1",
        "drupal/languagefield": "^1.7",
        "drupal/leaflet": "^2.1",
        "drupal/legal": "^2.0",
        "drupal/licenses": "^2.0",
        "drupal/linkit": "^6.0-dev",
        "drupal/matomo": "^1.13",
        "drupal/metatag": "^1.16",
        "drupal/oembed_providers": "^2.0",
        "drupal/paragraphs": "^1.12",
        "drupal/password_policy": "^3.0",
        "drupal/pathauto": "^1.8",
        "drupal/rabbit_hole": "^1.0-dev",
        "drupal/realname": "^2.0-dev",
        "drupal/redis": "^1.5",
        "drupal/role_delegation": "^1.1",
        "drupal/search_api": "^1.21",
        "drupal/similarterms": "^1.5",
        "drupal/slick": "^2.5",
        "drupal/slick_entityreference": "^2.0",
        "drupal/smart_date": "^3.5",
        "drupal/smtp": "^1.0",
        "drupal/social_media_links": "^2.8",
        "drupal/svg_image": "^1.15",
        "drupal/token": "^1.10",
        "drupal/twig_tweak": "^3.1",
        "drupal/viewfield": "^3.0@beta",
        "drupal/views_add_button": "^2.0",
        "drupal/views_autocomplete_filters": "^1.3",
        "drupal/views_bulk_operations": "^4.0",
        "drupal/views_infinite_scroll": "^2.0",
        "geocoder-php/nominatim-provider": "^5.5",
        "nodespark/des-connector": "^7-dev",
        "npm-asset/slick-carousel": "1.8.0",
        "predis/predis": "^1.1",
        "ruflin/elastica": "dev-master"
    },
    "require-dev": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/config_inspector": "^2.0",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drupal/devel": "^4.1",
        "drupal/stage_file_proxy": "^1.2",
        "drush/drush": "^10.6",
        "npm-asset/jquery-ui-touch-punch": "^0.2.3",
        "oomphinc/composer-installers-extender": "^2.0",
        "openculturas/openculturas-profile": "*"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[project-root]/.gitattributes": false,
                "[web-root]/example.gitignore": false,
                "[web-root]/INSTALL.txt": false,
                "[web-root]/README.md": false,
                "[web-root]/.gitignore": false,
                "[web-root]/sites/README.txt": false,
                "[web-root]/modules/README.txt": false,
                "[web-root]/profiles/README.txt": false,
                "[web-root]/themes/README.txt": false,
                "[web-root]/sites/development.services.yml": {
                    "mode": "replace",
                    "path": "assets/development.services.yml",
                    "overwrite": false
                }
            }
        },
        "installer-types": [
            "npm-asset",
            "bower-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "vendor:npm-asset",
                "vendor:bower-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patches": {
            "drupal/smtp": {
                "Issue #3248547: Remove after install messages": "https://www.drupal.org/files/issues/2021-11-09/3248547-2-remove-after-install-messages.patch"
            },
            "drupal/flag": {
                "Issue #3165081: Destination URL not set when deleting flag through Field Entry Form": "https://www.drupal.org/files/issues/2020-09-28/flag-destination-url-not-set-3165081-2.patch",
                "Issue #3260640: A11y: Send the status message via drupal.announce": "https://www.drupal.org/files/issues/2022-01-26/flag-3260640-ajax-link-message-announce-3.patch"
            },
            "drupal/focal_point": {
                "Issue #2830678: Limit image styles on preview page": "https://www.drupal.org/files/issues/2021-08-13/2830678-29.patch"
            },
            "drupal/address_display": {
                "Issue #3260530: Config schema for field formatter is missing": "https://www.drupal.org/files/issues/2022-01-26/address_display-3260530-2.patch"
            }
        }
    }
}
