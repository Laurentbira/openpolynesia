        "drupal/media_library_edit": "^2.2",
        "drupal/metatag": "^1.16",
        "drupal/node_co_authors": "^1.0@beta",
        "drupal/oembed_providers": "^2.0",
        "drupal/paragraphs": "^1.13",
        "drupal/password_policy": "^3.0",
        "drupal/pathauto": "^1.8",
        "drupal/rabbit_hole": "^1.0-dev",
        "drupal/realname": "^2.0-dev",
        "drupal/redirect": "^1.7",
        "drupal/role_delegation": "^1.1",
        "drupal/search_api": "^1.21",
        "drupal/similarterms": "^1.5",
        "drupal/smart_date": "^3.5",
        "drupal/smtp": "^1.0",
        "drupal/social_media_links": "^2.8",
        "drupal/svg_image": "^1.15",
        "drupal/token": "^1.10",
        "drupal/transliterate_filenames": "^2.0",
        "drupal/twig_tweak": "^3.1",
        "drupal/viewfield": "^3.0@beta",
        "drupal/views_add_button": "^2.0",
        "drupal/views_autocomplete_filters": "^1.3",
        "drupal/views_bulk_operations": "^4.1",
        "drupal/views_infinite_scroll": "^2.0",
        "dynamicweb/swiffy-slider": "^1.4",
        "elmarquis/leaflet.gesture-handling": "^1.2",
